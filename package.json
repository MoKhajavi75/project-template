{
  "name": "project-template",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "yarn clean && yarn build && yarn execute",
    "test": "yarn clean && yarn build && jest --no-cache",
    "clean": "rm -rf dist coverage",
    "build": "tsc -p tsconfig.json",
    "execute": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/index.js ./config.yml",
    "dev": "nodemon",
    "lint": "eslint .",
    "prettier": "prettier -c 'src/**/*.{ts,js,md,json}'",
    "prettier-fix": "prettier -w 'src/**/*.{ts,js,md,json}'",
    "coverage": "typescript-coverage-report -o coverage",
    "mirror-config": "sh scripts/mirror_config.sh",
    "prepare": "husky install"
  },
  "author": "MohamadKh75",
  "license": "MIT",
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@trivago/prettier-plugin-sort-imports": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/js-yaml": "^4.0.1",
    "@types/node": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.26.0",
    "husky": "^6.0.0",
    "jest": "^26",
    "nodemon": "^2",
    "prettier": "^2.3.0",
    "ts-node": "^9",
    "tsconfig-paths": "^3",
    "typescript": "^4.2.4",
    "typescript-coverage-report": "^0.6.0"
  }
}
